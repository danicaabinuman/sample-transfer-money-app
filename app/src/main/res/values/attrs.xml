<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AutoFormatEditText">
        <attr name="isDecimal" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="QrScannerView">
        <attr name="moving_bar" format="reference" />
        <attr name="bar_width" format="dimension" />
        <attr name="bar_height" format="dimension" />
        <attr name="bar_margin" format="dimension" />
        <attr name="moving_speed" format="integer" />

        <attr name="side_bg" format="reference" />
    </declare-styleable>

    <declare-styleable name="QRContainer">
        <attr name="outside_color" format="color" />
    </declare-styleable>

    <declare-styleable name="EasyMoneyWidgets">
        <attr name="currency_symbol" format="string" />
        <attr name="show_currency" format="boolean" />
        <attr name="show_commas" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CurrencyEditText">
        <attr name="currencySymbol" format="string"/>
        <attr name="localeTag" format="string"/>
        <attr name="useCurrencySymbolAsHint" format="boolean"/>
    </declare-styleable>


    <attr name="colorHighlight" format="color" />
    <attr name="colorText" format="color" />
    <attr name="colorImage" format="color" />
    <attr name="bottomSheetBackground" format="reference|color" />
    <attr name="appLogo" format="reference" />
    <attr name="appShareLogo" format="reference" />
    <attr name="appBackground" format="reference" />
    <attr name="iconToolbarColor" format="color" />
    <attr name="toolbarColor" format="color" />
    <attr name="appBarTheme" format="reference" />
    <attr name="colorAccentApp" format="color" />
    <attr name="startColor" format="color" />
    <attr name="startDisabledColor" format="color" />
    <attr name="endColor" format="color" />
    <attr name="endDisabledColor" format="color" />
    <attr name="disabledBGColor" format="color" />


</resources>